{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "fileSecrets": [
        {
          "secret": "GOOGLE_SERVICES_JSON",
          "target": "./google-services.json"
        }
      ]
    }
  },
  "submit": {
    "production": {}
  }
}
