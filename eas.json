{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "fileSecrets": {
        "googleServicesJson": {
          "path": "./google-services.json",
          "secret": "GOOGLE_SERVICES_JSON"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
